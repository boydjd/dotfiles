term screen-256color-s
screen -t irssi 0 irssi
screen -t ranger 0 bash -i -c "ranger"
screen -t bash 0 bash 
screen -t openfisma 0 bash -c "cd /home/josh/src/openfisma; bash"
screen -t mysql 0 mysql -u root 
select bash
autodetach on
startup_message off
defscrollback 30000
vbell off
hardstatus alwayslastline
hardstatus string '%{= w}%-w%{=b b}(%{b}%n %t%{= b})%{= w} %+w %{= g} %= %{g} %{=b R}%H%{= g} - %{=b Y}%l%{= g} - %{=b B}%M %d %C %a'
nonblock on
bindkey -k k5 next
